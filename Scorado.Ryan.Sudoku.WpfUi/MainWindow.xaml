<Window x:Class="Scorado.Ryan.Sudoku.WpfUi.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Scorado.Ryan.Sudoku.WpfUi"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBox x:Name="xPosition"></TextBox>
            <TextBox x:Name="yPosition"></TextBox>
            <TextBox x:Name="puzzleValue"></TextBox>
            <Button x:Name="get" Content="Set" Click="Button_Click"></Button>
            <DataGrid />
            <Button x:Name="set" Content="Get" Click="Button_Click"></Button>
        </StackPanel>
    </Grid>
</Window>
